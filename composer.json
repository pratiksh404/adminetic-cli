{
    "name": "adminetic/cli",
    "description": "Command line interface for adminetic admin panel.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://github.com/pratiksh404/adminetic-cli",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/pratiksh404/adminetic-cli/issues",
        "source": "https://github.com/pratiksh404/adminetic-cli"
    },
    "authors": [
        {
            "name": "Pratik Shrestha",
            "email": "pratikdai@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel-zero/framework": "^9.0",
        "nunomaduro/termwind": "^1.3",
        "pratiksh/adminetic": "^1.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["adminetic"]
}
